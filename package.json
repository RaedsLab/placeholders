{
  "name": "unsplash-placeholders",
  "private": false,
  "version": "0.0.1",
  "description": "Generate placeholders using unsplash's API",
  "license": "MIT",
  "author": {
    "name": "Raed Chammam",
    "email": "raed.chammam@gmail.com",
    "url": "https://raed.dev"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "lint": "eslint --ext .js,.ts ./src ",
    "lint:fix": "yarn lint --fix",
    "lint:ts": "tsc --noEmit",
    "dev": "tsc-watch --noClear --onSuccess \"node --require dotenv/config ./dist/start.js\"",
    "build": "yarn clean && tsc",
    "start": "node --require dotenv/config ./dist/start.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "cross-fetch": "^3.1.4",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "http-status-codes": "^2.1.4",
    "morgan": "^1.10.0",
    "node-fetch": "^3.1.0",
    "request": "^2.88.2",
    "slugify": "^1.6.3",
    "stoppable": "^1.1.0",
    "unsplash-js": "^7.0.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/eslint": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.9",
    "@types/node-fetch": "2",
    "@types/request": "^2.48.7",
    "@types/stoppable": "^1.1.1",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "tsc-watch": "^4.5.0",
    "typescript": "^4.5.2"
  }
}
